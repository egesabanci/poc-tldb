query: (insert | range | single)

STRING: /[a-zA-Z0-9]+/
NUMBER: /[0-9.]+/

SENDER: STRING 
RECIPIENT: STRING
AMOUNT: NUMBER
TIMESTAMP: NUMBER

?insert: "insert" (SENDER "->" RECIPIENT ":" AMOUNT)
?single: "single" ("->" TIMESTAMP)
?range: "range" (TIMESTAMP ".." TIMESTAMP)

%import common.WS
%ignore WS